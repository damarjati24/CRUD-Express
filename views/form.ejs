
<h1>Add New Contact</h1>
    <form action="/add" method="POST">
        <label for="name">Name:</label><br>
        <input type="text" value="<%= data.name %>" id="name" name="name" required><br><br>

        <label for="number">Number:</label><br>
        <input type="text" value="<%= data.number %>" id="number" name="number" required><br><br>

        <label for="email">Email:</label><br>
        <input type="email" value="<%= data.email %>" id="email" name="email" ><br><br>

        <input type="submit" value="Add Contact">
    </form>
    

